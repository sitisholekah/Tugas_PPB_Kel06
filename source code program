    private void jbpipitActionPerformed(java.awt.event.ActionEvent evt) {                                        
        jpisi.removeAll();
        jpisi.repaint();
        jpisi.revalidate();
        
        //menambahkan ke panel
        jpisi.add(jpipit);
        jpisi.repaint();
        jpisi.revalidate();
        String Nama = "Fitriya Nur Saidah";
        String NIM = "21120120130091";
        String Alamat = "Jepara";
        String Email = "nursaidahfitria@gmail.com";
        
        teksnama.setText(Nama);
        teksnim.setText(NIM);
        teksasal.setText(Alamat);
        teksemail.setText(Email);
    }                                       

